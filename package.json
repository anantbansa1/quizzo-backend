{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend server for the Pinegap dashboard",
  "main": "index.js",
  "type": "module",
  "esModuleInterop": true,
  "scripts": {
    "build-ts": "npx tsc",
    "copy-files": "copyfiles src/**/*.json package.json dist/",
    "build": "rimraf dist && npm run build-ts && resolve-tspaths && npm run copy-files ",
    "lint": "eslint --ext .ts,.tsx .",
    "lint-fix": "eslint --fix --ext .ts,.tsx .",
    "pretty": "npx prettier --write .",
    "pretty-check": "npx prettier --check .",
    "serve": "node dist/index.js",
    "start": "npm run serve",
    "dev": "nodemon",
    "prepare": "husky",
    "typeorm": "tsx ./node_modules/typeorm/cli",
    "migration:generate": "source .env && npm run typeorm migration:generate -- -t $npm_config_name --dataSource src/config/ormconfig.pgap.ts -p ./src/svc/migrations/migration",
    "migration:run": "source .env && npm run typeorm migration:run -- --dataSource src/config/ormconfig.pgap.ts",
    "migration:revert": "source .env && npm run typeorm migration:revert -- --dataSource src/config/ormconfig.pgap.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.651.1",
    "@aws-sdk/client-mwaa": "^3.668.0",
    "@aws-sdk/client-s3": "^3.701.0",
    "@aws-sdk/lib-dynamodb": "^3.651.1",
    "@langchain/core": "^0.3.3",
    "@langchain/openai": "^0.3.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.0",
    "fuzzball": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mixpanel": "^0.18.0",
    "moment-timezone": "^0.5.46",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "openai": "^4.62.1",
    "pg": "^8.13.0",
    "typeorm": "^0.3.20",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.5",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "^6.4.16",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "google-auth-library": "^9.14.2",
    "googleapis": "^144.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "resolve-tspaths": "^0.8.19",
    "rimraf": "^6.0.1",
    "tmp": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --ext .ts,.tsx .",
    "*.{ts,css,md}": "npx prettier --write ."
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "dist"
    ],
    "ext": "ts,json",
    "exec": "tsx --env-file=.env ./index.ts"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
